{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "base_denom",
    "local_denom",
    "lock_period",
    "pool_denom",
    "pool_id",
    "quote_denom",
    "return_source_channel",
    "transfer_channel"
  ],
  "properties": {
    "base_denom": {
      "type": "string"
    },
    "local_denom": {
      "type": "string"
    },
    "lock_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "pool_denom": {
      "type": "string"
    },
    "pool_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "quote_denom": {
      "type": "string"
    },
    "return_source_channel": {
      "type": "string"
    },
    "transfer_channel": {
      "type": "string"
    }
  }
}
